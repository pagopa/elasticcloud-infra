{
  "kubernetes.scheduler": {
    "enabled": false,
    "vars": {
      "bearer_token_file": "/var/run/secrets/kubernetes.io/serviceaccount/token",
      "hosts": [
        "https://0.0.0.0:10259"
      ],
      "period": "10s",
      "ssl.verification_mode": "none",
      "scheduler_label_key": "component",
      "scheduler_label_value": "kube-scheduler"
    }
  }
}