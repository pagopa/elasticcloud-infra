{
  "displayName": "ApiConfig",
  "indexTemplate": {
    "apiconfig": {
      "indexPatterns": [
        "logs-pagopaapiconfig-postgresql",
        "logs-pagopaapiconfig-oracle",
        "logs-apiconfig-selfcare-integration-microservice-chart",
        "logs-cache-oracle",
        "logs-cache-postgresql",
        "logs-cache-replica-oracle",
        "logs-cache-replica-postgresql",

        "logs-apiconfig"
      ],
      "customComponent": "basic-pipeline-lifecycle@custom",
      "ingestPipeline": "convert_responseTime_httpCode_with_failure"
    }
  },
  "dataStream": [
     "logs-pagopaapiconfig-postgresql",
     "logs-pagopaapiconfig-oracle",
     "logs-apiconfig-selfcare-integration-microservice-chart",
     "logs-cache-oracle",
     "logs-cache-postgresql",
     "logs-cache-replica-oracle",
     "logs-cache-replica-postgresql",

     "logs-apiconfig"
  ],
  "dataView": {
    "indexIdentifiers": [
      "logs-pagopaapiconfig-postgresql*",
      "logs-pagopaapiconfig-oracle*",
      "logs-apiconfig-selfcare-integration-microservice-chart*",
      "logs-cache-oracle*",
      "logs-cache-postgresql*",
      "logs-cache-replica-oracle*",
      "logs-cache-replica-postgresql*",

      "logs-apiconfig*"
    ]
  }
}
