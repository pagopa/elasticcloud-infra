{
  "processors": [
    {
      "json": {
        "field": "message",
        "add_to_root": true
      }
    },
    {
      "convert": {
        "field": "mdc.responseTime",
        "type": "long",
        "target_field": "response_time",
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "convert": {
        "field": "mdc.httpCode",
        "type": "integer",
        "target_field": "http_code",
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "json": {
        "field": "mdc.args",
        "target_field": "arguments",
        "ignore_failure": true
      }
    },
    {
      "json": {
        "field": "mdc.response",
        "ignore_failure": true
      }
    }
  ],
  "onFailure": [
    {
      "set": {
        "field": "ingest_failure",
        "value": "true"
      }
    }
  ]

}